    <DataSource Name="DataSource1">
      <ConnectionProperties>
        <DataProvider>SQL</DataProvider>
        <ConnectString>Data Source=TECHNO-497\SQLDEV19;Initial Catalog=PALMSPRODDB</ConnectString>
        <Prompt>Specify a user name and password for data source DataSource1:</Prompt>
      </ConnectionProperties>
      <rd:SecurityType>DataBase</rd:SecurityType>
      <rd:DataSourceID>4b82290c-6761-445a-9765-29c0bf853c6b</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DS_Query">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Period">
            <Value>=Parameters!Period.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Line">
            <Value>=Parameters!Line.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Date">
            <Value>=Parameters!Date.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT DISTINCT
    FORMAT(CAST(PAD.[Date] AS DATE), 'yyyy-MM-dd') AS [Date],
    SH.[ShiftName] AS [Shift],
    LEM.[LineName] AS [Line],
    SKU.[Model] AS [Model],
    BTM.[DieSet] AS [Bom_Type],
    PAD.[PlanLotSize] AS [Plan_Lot_Size],
    PAD.[ActualLotsize] AS [Actual_Lot_Size],
    CAST(ROUND(CAST(PAD.[LoadTime] AS DECIMAL(10,2)) / 60, 0) AS INT) AS [Production_Time_Plan],
    CAST(ROUND(CAST(PAD.[ActualLoadTime] AS DECIMAL(10,2)) / 60, 0) AS INT) AS [Actual_Production_Time]
FROM
    [SSPCSdbo].[PatternActualData] AS PAD
LEFT JOIN
    [dbo].[ShiftHeader] AS SH ON PAD.ShiftID = SH.ShiftID
LEFT JOIN
    [SSPCSdbo].[LineEfficencyMaster] AS LEM ON PAD.LineID = LEM.LineID
LEFT JOIN
    [SSPCSdbo].[BOMTypeMaster] AS BTM ON PAD.DieSetID = BTM.DieSetID
LEFT JOIN
    [dbo].[SKU] AS SKU ON PAD.DieSetID = SKU.SKUID
WHERE
    PAD.[Status] IS NOT NULL
    AND LEM.[LineName] = @Line
    AND (
        (@Date IS NOT NULL AND CAST(PAD.[Date] AS DATE) = @Date)
        OR
        (@Date IS NULL AND @Period IS NOT NULL AND @Period &lt;&gt; '' AND
            CASE @Period
                WHEN 'Today' THEN 
                    CASE WHEN CAST(PAD.[Date] AS DATE) = CAST(GETDATE() AS DATE) THEN 1 ELSE 0 END
                WHEN 'Yesterday' THEN 
                    CASE WHEN CAST(PAD.[Date] AS DATE) = DATEADD(DAY, -1, CAST(GETDATE() AS DATE)) THEN 1 ELSE 0 END
                WHEN 'Last Week' THEN 
                    CASE WHEN CAST(PAD.[Date] AS DATE) BETWEEN DATEADD(DAY, -7, CAST(GETDATE() AS DATE)) AND CAST(GETDATE() AS DATE) THEN 1 ELSE 0 END
                WHEN 'Last Month' THEN 
                    CASE WHEN CAST(PAD.[Date] AS DATE) BETWEEN DATEADD(MONTH, -1, CAST(GETDATE() AS DATE)) AND CAST(GETDATE() AS DATE) THEN 1 ELSE 0 END
                ELSE 0
            END = 1
        )
    );</CommandText>
      </Query>
      <Fields>
        <Field Name="Date">
          <DataField>Date</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Shift">
          <DataField>Shift</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Line">
          <DataField>Line</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Model">
          <DataField>Model</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Bom_Type">
          <DataField>Bom_Type</DataField>
