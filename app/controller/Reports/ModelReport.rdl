        </Field>
        <Field Name="Plan_Lot_Size">
          <DataField>Plan_Lot_Size</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Actual_Lot_Size">
          <DataField>Actual_Lot_Size</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Production_Time_Plan">
          <DataField>Production_Time_Plan</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Actual_Production_Time">
          <DataField>Actual_Production_Time</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DS_Date">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT DISTINCT 
    FORMAT(CAST(PAD.[Date] AS DATE), 'yyyy-MM-dd') AS [Date] 
FROM [SSPCSdbo].[PatternActualData] AS PAD 
WHERE PAD.[Date] IS NOT NULL
ORDER BY [Date] DESC;</CommandText>
      </Query>
      <Fields>
        <Field Name="Date">
          <DataField>Date</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DS_Line">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT DISTINCT 
    LEM.[LineID],
    LEM.[LineName]
FROM 
    [SSPCSdbo].[LineEfficencyMaster] AS LEM
WHERE 
    LEM.[LineName] IS NOT NULL
ORDER BY 
    LEM.[LineName]</CommandText>
      </Query>
      <Fields>
        <Field Name="LineName">
          <DataField>LineName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DS_Period">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT PeriodText, PeriodValue FROM (
    SELECT 'Today' as PeriodText, 1 as PeriodValue
    UNION ALL
    SELECT 'Yesterday', 2
    UNION ALL
    SELECT 'Last Week', 3
    UNION ALL
    SELECT 'Last Month', 4
    UNION ALL
    SELECT 'NULL' AS PeriodText, NULL AS PeriodValue  -- Adding NULL Option
) AS Periods;
</CommandText>
      </Query>
      <Fields>
        <Field Name="PeriodText">
          <DataField>PeriodText</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="PeriodValue">
          <DataField>PeriodValue</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
