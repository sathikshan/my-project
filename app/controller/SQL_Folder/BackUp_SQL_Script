CREATE VIEW SSPCSdbo.v_BOMTypeMaster AS
    SELECT
      [DieSetID]
      ,[DieSet]
      ,[DieID]
      ,[NoOfProcess]
      ,[UDTime]
      ,[CTTime]
      ,[QCTime]
      ,[MaterialChangeoverTimePerChangeOverTime]
      ,[PalletChangeoverTimePerChangeover]
      ,[DieStorageBay]
      ,[SDTime]
      ,[NoOfPalletPerCycle]
      ,[MaterialOrderTriggerInSec]
      ,[ProductionTriggerInSec]
      ,[ReferenceFileName]
      ,[CreatedBy]
      ,[CreatedDateTime]
      ,[ModifiedBy]
      ,[ModifiedDateTime]
  FROM [SSPCSdbo].[BOMTypeMaster]


  
INSERT INTO [dbo].[ViewScreenMapping] 

    ([ViewName], [CreatedBy], [CreatedDate], [ModifiedBy], [ModifiedDate], [ScreenID], [ScreenName], [ViewDisplayName], [CustomViewsID])

VALUES 

    ('SSPCSdbo.v_BOMTypeMaster', '1', GETDATE(), NULL, NULL, 440, NULL, 'BomtypeMasterview', NULL);

	Delete  from [dbo].[ViewScreenMapping]  where ViewName = 'SSPCSdbo.v_BOMTypeMaster'



