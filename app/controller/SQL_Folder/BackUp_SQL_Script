ALTER VIEW SSPCSdbo.v_PlannedLineStopMaster AS
SELECT 
    plsm.*,
    sc.SKUCategoryCode AS Line,
    sh.ShiftCode
FROM [SSPCSdbo].[PlannedLineStopMaster] plsm
LEFT JOIN [dbo].[SKUCategory] sc 
    ON plsm.LineID = sc.SKUCategoryID
LEFT JOIN [dbo].[ShiftHeader] sh 
    ON plsm.ShiftID = sh.ShiftId


	ALTER VIEW SSPCSdbo.v_PatternActualData AS
SELECT 
    pad.*,
    btm.DieSet,
    sc.SKUCategoryCode AS Line,
    sh.ShiftCode
FROM [SSPCSdbo].[PatternActualData] pad
LEFT JOIN [SSPCSdbo].[BOMTypeMaster] btm 
    ON pad.DieSetID = btm.DieSetID
LEFT JOIN [dbo].[SKUCategory] sc 
    ON pad.LineID = sc.SKUCategoryID
LEFT JOIN [dbo].[ShiftHeader] sh 
    ON pad.ShiftID = sh.ShiftId

ALTER VIEW SSPCSdbo.v_PatternDataInterpretation AS
SELECT 
    pdi.*,
    btm.DieSet,
    sc.SKUCategoryCode AS Line,
    sh.ShiftCode
FROM [SSPCSdbo].[PatternDataInterpretation] pdi
LEFT JOIN [SSPCSdbo].[BOMTypeMaster] btm 
    ON pdi.DieSetID = btm.DieSetID
LEFT JOIN [dbo].[SKUCategory] sc 
    ON pdi.LineID = sc.SKUCategoryID
LEFT JOIN [dbo].[ShiftHeader] sh 
    ON pdi.ShiftID = sh.ShiftId


ALTER VIEW SSPCSdbo.v_PatternRawDataUpload AS
SELECT 
    pru.*,
    sc.SKUCategoryCode AS LineCode
FROM [SSPCSdbo].[PatternRawDataUpload] pru
LEFT JOIN [dbo].[SKUCategory] sc 
    ON pru.LineName = sc.SKUCategoryCode


ALTER VIEW SSPCSdbo.v_PlanLotsizeCalculation AS
SELECT 
    plc.*,
    btm.DieSet,
    sku.SKUCode AS KitItemCode
FROM [SSPCSdbo].[PlanLotsizeCalculation] plc
LEFT JOIN [SSPCSdbo].[BOMTypeMaster] btm 
    ON plc.DieSetID = btm.DieSetID
LEFT JOIN [dbo].[SKU] sku 
    ON plc.kitItemId = sku.SKUID



ALTER VIEW SSPCSdbo.v_PQDataUpload AS
SELECT 
    pq.*,
    btm.DieSet AS BOMTypeDescription,
    sc.SKUCategoryCode AS LineCode
FROM [SSPCSdbo].[PQDataUpload] pq
LEFT JOIN [SSPCSdbo].[BOMTypeMaster] btm 
    ON pq.DieSet = btm.DieSet
LEFT JOIN [dbo].[SKUCategory] sc 
    ON pq.LineName = sc.SKUCategoryCode


